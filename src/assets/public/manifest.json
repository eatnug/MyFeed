{
  "name": "MyExtension",
  "description": "My chrome extension built with react",
  "version": "1.0.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/icons/icon-16.png",
      "32": "/icons/icon-32.png"
    }
  },
  "icons": {
    "16": "/icons/icon-16.png",
    "32": "/icons/icon-32.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  }
}
